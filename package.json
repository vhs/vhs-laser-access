{
  "name": "laser-access",
  "version": "0.1.0",
  "description": "VHS Laser Access",
  "scripts": {
    "prepare": "wireit",
    "start": "wireit",
    "watch": "wireit",
    "test": "wireit"
  },
  "dependencies": {
    "bluebird": "^2.9.14",
    "crypto-js": "^4.2.0",
    "debug": "~4.4.0",
    "express": "~4.21.2",
    "jade": "^1.9.2",
    "mqtt": "^4.3.7",
    "onoff": "^6.0.3",
    "promise": "~6.1.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "socket.io": "^4.8.1",
    "superagent": "~10.1.1",
    "superagent-promise": "^0.2.0",
    "supervisor": "~0.6.0",
    "underscore": "^1.8.2"
  },
  "devDependencies": {
    "@prettier/plugin-pug": "^3.2.0",
    "@tyisi/config-eslint": "^4.0.0",
    "@tyisi/config-prettier": "^1.0.1",
    "@types/gulp-mocha": "^0.0.37",
    "bower": "^1.8.14",
    "chai": "^2.1.1",
    "chai-as-promised": "^4.3.0",
    "eslint": "^9.16.0",
    "gulp": "^5.0.1",
    "gulp-istanbul": "^1.1.3",
    "gulp-jshint": "^2.1.0",
    "gulp-mocha": "^5.0.0",
    "jshint": "^2.13.6",
    "jshint-stylish": "^2.2.1",
    "mocha": "^11.7.4",
    "prettier": "^3.4.2",
    "sinon": "^21.0.0",
    "supertest": "^7.0.0",
    "supertest-as-promised": "^1.0.0",
    "wireit": "^0.14.9"
  },
  "jshintConfig": {
    "node": true,
    "globalstrict": true,
    "globals": {
      "describe": false,
      "it": false,
      "before": false,
      "beforeEach": false,
      "after": false,
      "afterEach": false
    }
  },
  "resolutions": {
    "graceful-fs": "^4.2.11"
  },
  "author": "Garth Cumming",
  "license": "ISC",
  "wireit": {
    "test": {
      "command": "node_modules/.bin/gulp test"
    },
    "watch": {
      "command": "node_modules/.bin/supervisor bin/www",
      "service": true
    },
    "start": {
      "command": "node_modules/.bin/supervisor -w app.js bin/www",
      "service": true
    },
    "prepare": {
      "dependencies": [
        "bower:install"
      ]
    },
    "bower:install": {
      "command": "bower install"
    }
  }
}
