{
  "name": "laser-access",
  "version": "0.1.0",
  "description": "VHS Laser Access",
  "scripts": {
    "check:format": "wireit",
    "format": "wireit",
    "lint": "wireit",
    "prepare": "wireit",
    "start": "wireit",
    "test": "wireit",
    "unittest": "wireit",
    "watch": "wireit"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "crypto-js": "^4.2.0",
    "debug": "~4.4.0",
    "express": "~4.21.2",
    "express-session": "^1.18.1",
    "morgan": "^1.10.0",
    "mqtt": "^5.10.3",
    "nyc": "^17.1.0",
    "onoff": "^6.0.3",
    "passport": "^0.7.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^2.0.0",
    "passport-slack": "git+https://github.com/garthomite/passport-slack.git",
    "pug": "^3.0.3",
    "socket.io": "^4.8.1",
    "superagent": "~10.1.1",
    "superagent-promise": "^1.1.0",
    "supervisor": "~0.12.0",
    "underscore": "^1.13.7",
    "wireit": "^0.14.9"
  },
  "devDependencies": {
    "@prettier/plugin-pug": "^3.2.0",
    "@tyisi/config-eslint": "^4.0.0",
    "@tyisi/config-prettier": "^1.0.1",
    "bower": "^1.8.14",
    "chai": "^4.5.0",
    "chai-as-promised": "^7.1.2",
    "eslint": "^9.17.0",
    "gulp": "^5.0.0",
    "gulp-istanbul": "^1.1.3",
    "gulp-jshint": "^2.1.0",
    "gulp-mocha": "^10.0.1",
    "jshint-stylish": "^2.2.1",
    "mocha": "^11.0.1",
    "prettier": "^3.4.2",
    "sinon": "^19.0.2",
    "supertest": "^7.0.0"
  },
  "jshintConfig": {
    "node": true,
    "globalstrict": true,
    "globals": {
      "describe": false,
      "it": false,
      "before": false,
      "beforeEach": false,
      "after": false,
      "afterEach": false
    }
  },
  "author": "Garth Cumming",
  "license": "ISC",
  "wireit": {
    "lint": {
      "dependencies": [
        "lint:eslint",
        "lint:prettier"
      ]
    },
    "lint:eslint": {
      "command": "eslint --fix ./*.js routes/**/*.js test/**/*.js"
    },
    "lint:prettier": {
      "command": "prettier -w ."
    },
    "prepare:bower:install": {
      "command": "bower install"
    },
    "prepare": {
      "dependencies": [
        "prepare:bower:install"
      ]
    },
    "start": {
      "command": "node_modules/.bin/supervisor -w app.js bin/www",
      "service": true
    },
    "test": {
      "command": "mocha test/*.test.js"
    },
    "unittest": {
      "command": "nyc npm run test"
    },
    "watch": {
      "command": "node_modules/.bin/supervisor bin/www",
      "service": true
    }
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c"
}
